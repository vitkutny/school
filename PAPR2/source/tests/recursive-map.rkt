(include "../functions/recursive-map.rkt")

(equal? (recursive-map (lambda (x) (* 10 x)) '())
        '())
(equal? (recursive-map (lambda (x) (* 10 x)) '(1 2 3 4))
        '(10 20 30 40))
(equal? (recursive-map (lambda (x) (* 10 x)) '(1 2 (3 4 (5 6) 7) 8))
        '(10 20 (30 40 (50 60) 70) 80))
(equal? (recursive-map (lambda (x) (* 10 x)) (cons 1 (cons (list 2 3 4) 5)))
        (cons 10 (cons (list 20 30 40) 50)))